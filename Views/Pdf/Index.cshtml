@model Rotativa_Html_to_PDF.Models.Movies
@{
    Layout = null;
}

<h1>
    Make PDF file from HTML with Rotativa
</h1>

<p>
    <h2>List Action Movies</h2>
</p>

@foreach (var item in Model.Action)
{
    <div>
        <h3>@item.Name</h3>
        <span>@item.Type</span>
        <span>@item.Year</span>
    </div>
}

<p>
    <h2>List Thriller Movies</h2>
</p>

@foreach (var item in Model.Thriller)
{
    <div>
        <h3>@item.Name</h3>
        <span>@item.Type</span>
        <span>@item.Year</span>
    </div>
}
